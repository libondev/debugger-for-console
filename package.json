{
  "name": "debugger-for-console",
  "displayName": "Debugger for Console",
  "version": "0.4.0",
  "packageManager": "pnpm@6.35.1",
  "description": "Quickly create console debugging information for multiple languages.",
  "keywords": [],
  "license": "MIT",
  "author": "Libon <bon.li@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/libondev/debugger-for-console"
  },
  "main": "./dist/index.js",
  "publisher": "banlify",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "homepage": "https://github.com/libondev/debugger-for-console#readme",
  "bugs": {
    "url": "https://github.com/libondev/debugger-for-console/issues"
  },
  "scripts": {
    "build": "npm run typecheck && tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint . --fix",
    "vscode:prepublish": "nr build",
    "pack": "vsce package --no-dependencies",
    "release": "bumpp --commit --push --tag && nr publish",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@antfu/ni": "^0.21.5",
    "@babel/types": "^7.22.5",
    "@types/node": "^18.17.1",
    "@types/vscode": "1.80.0",
    "@vscode/vsce": "^2.19.0",
    "acorn": "^8.10.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.45.0",
    "pnpm": "^6.35.1",
    "rimraf": "^3.0.2",
    "tsup": "^7.1.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other",
    "Debuggers"
  ],
  "activationEvents": [
    "onCommand:debugger-for-console.create",
    "onCommand:debugger-for-console.before",
    "onCommand:debugger-for-console.remove",
    "onCommand:debugger-for-console.toggle"
  ],
  "contributes": {
    "commands": [
      {
        "title": "DebuggerForConsole: Insert a debug statement after it",
        "command": "debugger-for-console.create",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "title": "DebuggerForConsole: Insert a debug statement before it",
        "command": "debugger-for-console.before",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "title": "DebuggerForConsole: Toggle all debugger statements",
        "command": "debugger-for-console.toggle",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "title": "DebuggerForConsole: Remove all debugger statements",
        "command": "debugger-for-console.remove",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "title": "DebuggerForConsole: Update latest workbench configuration",
        "command": "debugger-for-console.update",
        "when": "editorTextFocus && !editorReadonly"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+down",
        "command": "debugger-for-console.create",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+shift+up",
        "command": "debugger-for-console.before",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+shift+/",
        "command": "debugger-for-console.toggle",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+shift+backspace",
        "command": "debugger-for-console.remove",
        "when": "editorTextFocus && !editorReadonly"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "debugger-for-console",
      "properties": {
        "debugger-for-console.semi": {
          "type": "boolean",
          "default": false,
          "description": "Insert the semicolon in the debug statement."
        },
        "debugger-for-console.emoji": {
          "type": "boolean",
          "default": true,
          "description": "Insert the random emoji in the debug statement."
        },
        "debugger-for-console.quote": {
          "type": "string",
          "enum": ["'", "\"", "`"],
          "default": "'",
          "description": "Insert the quoted text in the debug statement."
        },
        "debugger-for-console.fileName": {
          "type": "boolean",
          "default": true,
          "description": "Insert the filename in the debug statement."
        },
        "debugger-for-console.autoSave": {
          "type": "boolean",
          "default": false,
          "description": "Save the file after insert debugger statements."
        },
        "debugger-for-console.lineNumber": {
          "type": "boolean",
          "default": true,
          "description": "Insert the line number in the debug statement."
        },
        "debugger-for-console.symbols": {
          "type": "boolean",
          "default": false,
          "description": "Insert the scope symbols in the debug statement. \nTips: Enabling this option can make your debug statements very very very loooooong,\nbecause it represents your code call level."
        },
        "debugger-for-console.wrappers": {
          "type": "object",
          "description": "Wrapper method for debug statements, or set to debugger.",
          "default": {
            "php": "var_dump",
            "go": "fmt.Println",
            "csharp": "Console.Log",
            "javascript": "console.log",
            "default": "console.log"
          }
        }
      }
    }
  }
}
